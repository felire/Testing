{
  "name": "Challenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src",
    "coverage": "jest --coverage",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "force-clean": "npm run android:clean && rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:build": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew clean && ./gradlew assembleRelease && ./gradlew installRelease",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint-fix": "eslint src --fix",
    "lint-diff": "git diff --name-only --cached --relative | grep \\.js$ | xargs eslint",
    "precommit": "npm run lint-diff"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.3",
    "apisauce": "^1.1.1",
    "formik": "^1.5.4",
    "i18next": "^19.0.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.4",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-localize": "^1.3.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.0.0-alpha.11",
    "react-native-splash-screen": "^3.2.0",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.3.2",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "reactotron-apisauce": "^3.0.0",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "redux": "^4.0.4",
    "redux-recompose": "^2.2.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/runtime": "^7.7.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-react": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^3.0.9",
    "jest": "^24.5.0",
    "jest-react-native": "^18.0.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "^0.57.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "react-native-mock-render": "^0.1.6",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/setup/",
      "<rootDir>/__tests__/redux/store.js",
      "<rootDir>/__tests__/redux/utils.js",
      "<rootDir>/__tests__/responses/",
      "<rootDir>/__tests__/redux/items/responseExamples",
      "<rootDir>/__tests__/redux/services/service.js",
      "<rootDir>/__tests__/redux/screens/Home.js",
      "<rootDir>/__tests__/redux/items/actions.js",
      "<rootDir>/__tests__/validations/tests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/@react-native-community/async-storage/(?!(lib))"
    ]
  }
}
